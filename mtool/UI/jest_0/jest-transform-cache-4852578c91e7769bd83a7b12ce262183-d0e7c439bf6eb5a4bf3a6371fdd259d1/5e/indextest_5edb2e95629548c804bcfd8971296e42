  it("edits an email entry in the email list", async () => {
    const mock = new _axiosMockAdapter.default(_axios.default);
    mock.onGet("/api/v1.0/get_email_ids/").reply(200, [{
      active: 1,
      edit: false,
      email: "palak.k@samsung.com",
      selected: false
    }]);
