    renderComponent();
    const _wrapper11 = wrapper,
          asFragment = _wrapper11.asFragment,
          getByTitle = _wrapper11.getByTitle,
          getByPlaceholderText = _wrapper11.getByPlaceholderText;
    const addElement = getByTitle("Add");

